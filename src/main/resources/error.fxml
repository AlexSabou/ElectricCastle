<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="199.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/9.0.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.controller.InfoPopupController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="81.0" prefWidth="337.0" VBox.vgrow="ALWAYS">
         <children>
            <TextArea fx:id="infoArea" editable="false" layoutY="49.0" prefHeight="106.0" prefWidth="349.0" promptText="Error" wrapText="true" />
            <Label fx:id="labelTitle" alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="149.0" layoutY="14.0" text="Error" textAlignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button fx:id="btnOk" layoutX="149.0" layoutY="160.0" mnemonicParsing="false" onMouseClicked="#onOkClicked" prefHeight="25.0" prefWidth="52.0" text="Ok" />
         </children>
    </AnchorPane>
  </children>
</VBox>
