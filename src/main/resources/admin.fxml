<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.controller.AdminFrameController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="tableView" layoutX="15.0" layoutY="37.0" prefHeight="308.0" prefWidth="460.0" />
            <Button fx:id="btnAdd" layoutX="111.0" layoutY="353.0" mnemonicParsing="false" onMouseClicked="#onNewClicked" prefHeight="33.0" prefWidth="85.0" text="New" />
            <Button fx:id="btnUpdate" layoutX="390.0" layoutY="353.0" mnemonicParsing="false" onMouseClicked="#onUpdateClicked" prefHeight="33.0" prefWidth="85.0" text="Update" />
            <Button fx:id="btnDelete" layoutX="298.0" layoutY="353.0" mnemonicParsing="false" onMouseClicked="#onDeleteClicked" prefHeight="33.0" prefWidth="85.0" text="Delete" />
            <Button fx:id="btnReportCashier" layoutX="515.0" layoutY="37.0" mnemonicParsing="false" onMouseClicked="#onCashierRepClicked" prefHeight="33.0" prefWidth="111.0" text="Report/Cashier" />
            <Button fx:id="btnReportDays" layoutX="515.0" layoutY="82.0" mnemonicParsing="false" onMouseClicked="#onReportDaysClicked" prefHeight="33.0" prefWidth="111.0" text="Report/Days" />
            <Button fx:id="btnLogout" layoutX="515.0" layoutY="353.0" mnemonicParsing="false" onMouseClicked="#onLogoutClicked" prefHeight="33.0" prefWidth="111.0" text="Log out" />
            <Button fx:id="btnInsert" layoutX="203.0" layoutY="353.0" mnemonicParsing="false" onMouseClicked="#onInsertClicked" prefHeight="33.0" prefWidth="85.0" text="Insert" />
            <Label fx:id="labelCapacity" layoutX="515.0" layoutY="146.0" text="Capacity:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="tfCapacity" layoutX="515.0" layoutY="166.0" prefHeight="25.0" prefWidth="111.0" promptText="New capacity" />
            <Button fx:id="btnCapacity" layoutX="516.0" layoutY="196.0" mnemonicParsing="false" onMouseClicked="#onCapacityClicked" prefHeight="33.0" prefWidth="111.0" text="Update" />
            <Label layoutX="15.0" layoutY="357.0" text="User actions:">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
